{
	"env": {
		"es2021": true,
		"node": true
	},
	"ignorePatterns": ["dist"],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"plugin:editorconfig/noconflict"
	],
	"plugins": ["@typescript-eslint", "editorconfig", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {}
}
